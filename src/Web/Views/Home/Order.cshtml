@model OrderListViewModel
@{
    ViewData["Title"] = "Order";
}
<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">Tenant name</th>
            <th scope="col">Insert time</th>
            @foreach (var i in Model.OrderAttributeDtos)
            {
                <th scope="col">@i.Name</th>
            }
            <th scope="col">Edit</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var i in Model.OrderDtos)
        {
            <tr attrid="@i.Id">
                <td>@i.TenantName</td>
                <td>@i.InsertTime</td>
                @foreach (var j in Model.OrderAttributeDtos)
                {
                    <td>@i[j.Id]</td>
                }
                <td>
                    <button class="order-edit btn btn-info">Edit</button>
                </td>
            </tr>
        }
    </tbody>
</table>
<button id="createOrder" type="button" class="btn btn-info">Create order</button>

<script>
    window.onload = function () {
        //绑定元素事件
        $("#createOrder").click(function () {
            window.location = "/home/editorder"
        })
        $(".order-edit").click(function () {
            var id = $(this).parents("tr").attr("attrid")
            window.location = "/home/editorder?id=" + id
        })
    }
</script>